{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2021",
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "externalHelpers": true,
    "keepClassNames": true,
    "loose": false
  },
  "module": {
    "type": "commonjs",
    "strictMode": true,
    "noInterop": false
  },
  "sourceMaps": true,
  "minify": false,
  "env": {
    "targets": {
      "node": "20"
    }
  }
}
