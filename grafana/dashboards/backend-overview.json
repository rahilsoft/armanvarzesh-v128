{
  "title": "Arman Backend â€” Overview",
  "schemaVersion": 38,
  "version": 1,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "type": "stat",
      "title": "RPS (all)",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m]))"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "stat",
      "title": "Error rate (5xx %)",
      "targets": [
        {
          "expr": "100 * sum(rate(http_requests_total{status=~\"5..\"}[5m])) / clamp_min(sum(rate(http_requests_total[5m])), 1)"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "type": "stat",
      "title": "p95 latency (s)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_request_duration_seconds_bucket[5m])))"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Requests by status",
      "targets": [
        {
          "expr": "sum by (status) (rate(http_requests_total[5m]))"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 6
      }
    },
    {
      "type": "timeseries",
      "title": "Latency by status (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le,status) (rate(http_request_duration_seconds_bucket[5m])))"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 6
      }
    },
    {
      "type": "table",
      "title": "Top routes (RPS)",
      "targets": [
        {
          "expr": "topk(10, sum by (route,method) (rate(http_requests_total[5m])))"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 14
      }
    }
  ]
}