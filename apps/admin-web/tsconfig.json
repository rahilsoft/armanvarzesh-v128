{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@graphql/*": [
        "graphql/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@context/*": [
        "context/*"
      ],
      "@theme/*": [
        "theme/*"
      ],
      "coach-pwa/*": [
        "app/coach-pwa/*"
      ],
      "@coach-pwa/*": [
        "app/coach-pwa/*"
      ],
      "activity-subgraph/*": [
        "app/activity-subgraph/*"
      ],
      "@activity-subgraph/*": [
        "app/activity-subgraph/*"
      ],
      "user-app/*": [
        "app/user-app/*"
      ],
      "@user-app/*": [
        "app/user-app/*"
      ],
      "coach-app/*": [
        "app/coach-app/*"
      ],
      "@coach-app/*": [
        "app/coach-app/*"
      ],
      "media-worker/*": [
        "app/media-worker/*"
      ],
      "@media-worker/*": [
        "app/media-worker/*"
      ],
      "social-subgraph/*": [
        "app/social-subgraph/*"
      ],
      "@social-subgraph/*": [
        "app/social-subgraph/*"
      ],
      "backend/*": [
        "app/backend/*"
      ],
      "@backend/*": [
        "app/backend/*"
      ],
      "live-subgraph/*": [
        "app/live-subgraph/*"
      ],
      "@live-subgraph/*": [
        "app/live-subgraph/*"
      ],
      "admin/*": [
        "app/admin/*"
      ],
      "@admin/*": [
        "app/admin/*"
      ],
      "site/*": [
        "app/site/*"
      ],
      "@site/*": [
        "app/site/*"
      ],
      "user-pwa/*": [
        "app/user-pwa/*"
      ],
      "@user-pwa/*": [
        "app/user-pwa/*"
      ]
    },
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "src",
    "outDir": "dist",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "noPropertyAccessFromIndexSignature": true,
    "useUnknownInCatchVariables": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ],
  "extends": "../../tsconfig.base.json"
}
