{
  "name": "@arman/integration",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --clean",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "test": "vitest run",
    "clean": "rimraf dist || true",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "format": "prettier --write .",
    "test:e2e": "echo \"e2e placeholder\"",
    "dev": "nodemon --exec ts-node src/main.ts || echo \"dev script placeholder\"",
    "start": "node dist/main.js",
    "prepare": "husky install || true",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "start:prod": "node dist/main.js",
    "coverage": "jest --coverage --passWithNoTests",
    "sbom": "npx @cyclonedx/cyclonedx-npm --ignore-npm-errors --json --output-file sbom.json"
  },
  "dependencies": {
    "pg": "^8.12.0",
    "bullmq": "^5.7.5",
    "ioredis": "^5.4.1",
    "zod": "^4.1.8",
    "pino": "^9.3.2",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "typescript": "^5.6.2",
    "@types/node": "^20.12.12",
    "vitest": "^3.2.4",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.0",
    "tsup": "^8.0.1"
  },
  "engines": {
    "node": ">=20.0.0 <21",
    "pnpm": ">=9 <10"
  },
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false
}
