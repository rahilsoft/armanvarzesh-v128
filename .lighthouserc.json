{
  "ci": {
    "collect": { "url": ["http://localhost:3000/","http://localhost:3000/status","http://localhost:3000/marketplace"], "startServerCommand": "cd apps/site-web && pnpm start", "numberOfRuns": 1 },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", {"minScore": 0.85}],
        "categories:accessibility": ["warn", {"minScore": 0.9}]
      }
    },
    "upload": { "target": "temporary-public-storage" }
  }
}
